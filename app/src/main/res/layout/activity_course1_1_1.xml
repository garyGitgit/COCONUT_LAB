<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.gachon.app.course1_1.course1_1_1.Course1_1_1Activity"
    android:padding="20dp"
    android:orientation="vertical"
    android:weightSum="10">

    <include layout="@layout/progresscheck" />
    <com.gachon.app.helper.MyViewPager
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:id="@+id/course1_1_ViewPager">
    </com.gachon.app.helper.MyViewPager>

    <Button
        android:id="@+id/buttonGoNext1_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:text="다음" />
</LinearLayout>
